{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/product.schema.json",
    "title": "Product",
    "description": "A product from Acme's catalog",
    "type": "object",
    "properties": {
        "productId": {
            "description": "The unique identifier for a product",
            "type": 'button',
            "buttonLabel": 'Save',
            "conditions": [
              ['text', 'not_empty'],
              ['phone', '^', '1'],
            ],
            "target": "_top",
            "between": "asd",
            "after": "asda",
            "before": "asd",
            "columns": {
              "container": 3
            }
        }
    }
}